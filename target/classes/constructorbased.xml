<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <!--    <bean id="stud" class="com.emexo.spring.ioc.beanfactory.Student" />-->

    <bean id="student1" class="com.spring.practice.dependecyInjection.constructorBased.Student">
     <constructor-arg index="0" value="2"/>
        <constructor-arg index="1" value="Santosh"/>
        <constructor-arg index="2" ref="marks"/>
    </bean>
    <bean id="marks" class="com.spring.practice.dependecyInjection.constructorBased.Mark">
     <constructor-arg index="0" value="60"/>
        <constructor-arg index="1" value="70"/>
        <constructor-arg index="2" value="90"/>
    </bean>


</beans>