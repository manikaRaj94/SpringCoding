<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <!--    <bean id="stud" class="com.emexo.spring.ioc.beanfactory.Student" />-->

    <bean id="stud" class="com.spring.practice.dependecyInjection.setterbased.Student">
        <property name="rollNo" value="1"/>
        <property name="studentName" value="Manika"/>
        <property name="marks" ref="marks"/>
    </bean>
    <bean id="marks" class="com.spring.practice.dependecyInjection.setterbased.Marks">
            <property name="engMarks" value="80"/>
            <property name="mathMarks" value="90"/>
    </bean>


</beans>